<Window x:Class="DeleteTransactionGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeleteTransactionGUI"
        mc:Ignorable="d"
        Title="Удаление зависшей транзакции" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Content="Введите номер зависшей транзакции"></Label>
        <TextBlock Grid.Row="1"></TextBlock>
        <Button Grid.Row="2"></Button>
        <ListBox Grid.Row="3"></ListBox>
        <Button Grid.Row="4"></Button>
    </Grid>
</Window>
